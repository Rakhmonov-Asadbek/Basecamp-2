<div class="d-flex justify-content-center align-items-center"
  style="height:90vh;"
>
              <div class="col-md-10 col-lg-6 col-xl-7 d-flex align-items-center order-1 order-lg-2">

                <img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-registration/draw1.webp"
                  class="img-fluid" alt="Sample image">

              </div>
<div style="width:350px;">
<div class="text-center"><h2>Sign up</h2></div>
<div class="new">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="form">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true,  autocomplete: "email", class:"form-control" %>
  </div>

  <div class="form">
    <%= f.label :password %>
    <% if @minimum_password_length %>

    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", class:"form-control" %>
  </div>

  <div class="form">
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class:"form-control" %>
  </div>

  <div class="d-flex justify-content-center mt-5 mx-4 mb-10 mb-lg-4">
    <%= f.submit "Sign up", data:{ turbo: false }, class:"btn btn-info mt-2" %>
</div>
<div class="d-flex justify-content-center mx-4 mb-3 mb-lg-4">
<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name),class:"btn btn-success btn-lg"  %>
<% end %>
</div>

  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>

<style>

.new{
  /* padding:100px; */
      width: 350px;
    min-height: 500px;
    height: auto;
    border-radius: 5px;
    margin: 2% auto;
    box-shadow: 0 9px 50px hsl(20deg 67% 75% / 31%);
    padding: 2%;
    background-image: url("data:image/jpeg;base64,");
}
.form{
      width: 340px;
    height: 60px;
    margin-top: 5%;
    padding: 20px;
    font-size: 16px;
    font-family: 'Abel', sans-serif;
    color: #5E6472;
    outline: none;
    border: none;
    border-radius: 0px 5px 5px 0px;
    transition: 0.2s linear;
}

</style>

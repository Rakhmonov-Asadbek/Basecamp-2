<%= form_with(model: upl) do |form| %>
  <% if upl.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(upl.errors.count, "error") %> Prohibited this upl from being saved:</h2>

      <ul>
        <% upl.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :name, style: "display: block" %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :project_id, style: "display: block" %>
    <% @project = Project.find(params[:project_id]) %>
    <%= form.text_field :project_id, value: @project.id, type:  :hidden %>
  </div>

  <div>
    <%= form.file_field :fayl %>
  </div>


  <div>
    <%= form.submit %>
  </div>
<% end %>
